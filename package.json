{
  "name": "ra-data-treeql",
  "version": "0.3.6",
  "description": "PHP-CRUD-API / TreeQL Data Provider and automatic UI generator for react-admin",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "keywords": [
    "react-admin",
    "ra-data",
    "php-crud-api",
    "treeql",
    "sql-admin"
  ],
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "esm",
    "src"
  ],
  "authors": [
    "Javi Aguilar"
  ],
  "repository": "itsjavi/ra-data-treeql",
  "homepage": "https://github.com/itsjavi/ra-data-treeql#readme",
  "bugs": "https://github.com/itsjavi/ra-data-treeql/issues",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run build-cjs && npm run build-esm",
    "build-cjs": "rm -rf ./lib && tsc",
    "build-esm": "rm -rf ./esm && tsc --outDir esm --module es2015",
    "watch": "tsc --outDir esm --module es2015 --watch"
  },
  "dependencies": {
    "en-inflectors": "^1.0.12",
    "history": "^4.10.1",
    "query-string": "^7.1.0",
    "react-admin": "^3.19.6",
    "ra-core": "^3.19.6",
    "underscore.string": "^3.3.6"
  },
  "devDependencies": {
    "@types/history": "^4.7.11",
    "@types/node": "17.0.10",
    "@types/pluralize": "^0.0.29",
    "@types/query-string": "^6.3.0",
    "@types/react": "17.0.38",
    "@types/underscore.string": "^0.0.38",
    "eslint-plugin-react-hooks": "^4.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "4.5.5"
  }
}
